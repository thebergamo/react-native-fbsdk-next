{
  "name": "react-native-fbsdk-next",
  "version": "8.0.4",
  "description": "Facebook SDK support for React Native apps.",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/src/index.d.ts",
  "sideEffects": false,
  "author": {
    "name": "Marcos BÃ©rgamo"
  },
  "contributors": [
    {
      "name": "Zhuowen Deng"
    },
    {
      "name": "Chris Hackmann"
    },
    {
      "name": "Pouya Larjani"
    },
    {
      "name": "Cody Winton"
    },
