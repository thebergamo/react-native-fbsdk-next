{
  "name": "react-native-fbsdk-next",
  "version": "13.0.0",
  "description": "Facebook SDK support for React Native apps.",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/src/index.d.ts",
  "sideEffects": false,
  "author": {
    "name": "Marcos BÃ©rgamo"
  },
  "contributors": [
    {
      "name": "Zhuowen Deng"
    },
    {
      "name": "Chris Hackmann"
    },
